{
  "builds": [
    {
      "src": "image_converter/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
      "src": "sh build.sh"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "image_converter/wsgi.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    }
  ]
}
